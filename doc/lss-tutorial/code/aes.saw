import "AES.cry";

let {{ aesExtract x = join (reverse (groupBy`{32} (aesEncrypt (ct,key))))
             where [ct,key] = split x
    }};

let main = do {
  print "Loading LLVM implementation";
  f <- load_aig "aes.aig";

  print "Bitblasting Cryptol implementation";
  g <- bitblast {{ aesExtract }};

  print "Checking equivalence";
  res <- cec f g;
  print res;
};
